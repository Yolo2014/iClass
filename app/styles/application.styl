@import 'nib'

body
  // padding-top 60px
  // overflow hidden
  background url('../img/bg_main.png')

#main-container
  margin 0
  padding 0

blue
  color #1fb4da

aqua
  color #6ec8c7

green
  color #93c054

yellow
  color #ffbe00

orange
  color #f67d42

silver
  color #f1f1f1

gray
  color #d1d1d1

.icon
  display inline
  float left
  width 16px
  height 16px
  background url('../img/icon.png')
  margin-top 2px
  margin-right 5px

.icon-lg
  display inline
  float left
  width 32px
  height 32px
  background url('../img/icon.png')


.icon-err
  background-position 0 0

.icon-not
  background-position 0 -40px

.icon-qst
  background-position 0 -80px

.icon-warn
  background-position 0 -120px

.icon-tip
  background-position 0 -160px

.icon-ok
  background-position 0 -200px

.icon-msg
  background-position 0 -240px

